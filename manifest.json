{
  "name": "Field Filler",
  "version": "1.0.0",
  "description": "A Plugin to Chrome to fill specific inputs",
  "manifest_version": 3,
  "author": "Kaio Souza",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "assets/js/background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Field Filler",
    "default_icon": {
      "16": "./assets/images/icon.png",
      "32": "./assets/images/icon.png",
      "48": "./assets/images/icon.png",
      "128": "./assets/images/icon.png"
    }
  }
}